ans-serif',\n            fontSize: '16px',\n            lineHeight: '24px',\n            light: 400,\n            normal: 400,\n            bold: 700,\n        },\n    };\n    return (\n        <PostonentsProvider theme={theme}>\n            <Email\n                headLinks={[\n                    {\n                        type: 'link',\n                        props: {\n                            rel: 'stylesheet',\n                            href:\n                                'https://fonts.googleapis.com/css?family=Nunito+Sans:400,500,600,700',\n                        },\n                    },\n                ]}\n            >\n                <Header\n                    logo=\"https://d2q86h5pvn46dj.cloudfront.net/resources/cm_logo_blue.png\"\n                    logoWidth={150}\n                    style={{ marginBottom: 24, marginTop: 24 }}\n                />\n                <FullWidth style={{ marginBottom: 24 }}> {content}</FullWidth>\n                <Footer></Footer>\n            </Email>\n        </PostonentsProvider>\n    );\n};\n\nexport default OnboardingTemplate;\n","/Users/marian/Projects/email-templates/src/screens/home/home.screen.jsx",["65","66","67","68"],"/Users/marian/Projects/email-templates/src/emails/day8.js",["69"],"/Users/marian/Projects/email-templates/src/emails/day10.js",["70"],"/Users/marian/Projects/email-templates/src/email-components/Video.tsx",["71","72","73"],"/Users/marian/Projects/email-templates/src/emails/day9.js",["74"],"/Users/marian/Projects/email-templates/src/emails/day4.js",[],"/Users/marian/Projects/email-templates/src/email-components/Text.tsx",["75","76"],"/Users/marian/Projects/email-templates/src/reportWebVitals.ts",[],"/Users/marian/Projects/email-templates/src/screens/edit-email/edit-email.screen.tsx",["77","78","79","80","81","82","83"],"/Users/marian/Projects/email-templates/src/emails/day1.js",[],{"ruleId":"84","severity":1,"message":"85","line":2,"column":10,"nodeType":"86","messageId":"87","endL